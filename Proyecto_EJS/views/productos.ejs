<h2 style="color:blue;">Listado de Productos</h2>

<% if(hayProducto){ %>
<div class="table-responsive">
    <table class="table table-dark table-striped">
        <tr style="color: yellow;">
            <th>Titulo</th>
            <th>Precio</th>
            <th>Foto URL</th>
        </tr>
        <% for(var i=0; i<productos.length; i++) {%>    
        <tr>
            <td>
                <%=productos[i].title%>
            </td>
            <td>
                $<%=productos[i].price%>
            </td>
            <td><img width="50" src=<%=productos[i].thumbnail%> alt="not found"></td>           
        </tr>
        <%};%>
    </table>
</div>
<% } else{ %>
<h3 class="alert alert-danger">no se encontraron datos</h3>
<% } %>